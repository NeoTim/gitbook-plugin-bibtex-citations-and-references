{
  "name": "gitbook-plugin-bibtex-citations-and-references",
  "version": "0.5.1",
  "description": "ALPHA TEST. Plugin for Gitbook that handles citations and references. Expects bibtex references. Produces VERY BASIC Harvard-type inline references and bibliography.",
  "engines": {
    "gitbook": ">=2.0.0"
  },
  "gitbook": {
    "testprops": {
      "type": "string",
      "default": "a",
      "enum": [
        "a",
        "b"
      ]
    }
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austinjp/gitbook-plugin-bibtex-citations-and-references.git"
  },
  "keywords": [
    "gitbook",
    "bibtex",
    "citations",
    "references"
  ],
  "author": "austinjp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/austinjp/gitbook-plugin-bibtex-citations-and-references/issues"
  },
  "homepage": "https://github.com/austinjp/gitbook-plugin-bibtex-citations-and-references#readme",
  "dependencies": {
    "bibtex-parser": ">=0.0.0",
    "bibtex-to-csl-json": ">=0.0.12",
    "citeproc-js-node-patch": ">=0.0.2",
    "colors": ">=1.1.2"
  },
  "devDependencies": {
    "bib2json": "0.0.1",
    "bibtex-parse-js": "0.0.23",
    "bibtex-parser": ">=0.0.0",
    "bibtex-to-csl-json": ">=0.0.12",
    "citeproc-js-node": "0.0.2",
    "citeproc-js-node-patch": ">=0.0.2",
    "colors": ">=1.1.2",
    "glob": "^7.0.5",
